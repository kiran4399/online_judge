#include<stdio.h>
int main(){
    long long int n,arr[1000000];
    scanf("%lld",&n);
    long long int i;
    for(i=0;i<n;i++){
        scanf("%lld",&arr[i]);
    }
    long long int min=arr[0];
    for(i=0;i<n;i++){
        if(min>arr[i])
            min = arr[i];
    }
    printf("%lld\n",min);
    return 0;

}
